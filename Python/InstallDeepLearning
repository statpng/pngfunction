# https://theorydb.github.io/dev/2020/02/14/dev-dl-setting-local-python/

conda create -n Tensorflow2.0 python=3.7
# conda env remove --name Tensorflow2.0
conda activate Tensorflow2.0
conda install -n Tensorflow2.0 ipython notebook jupyter 

conda install -n Tensorflow2.0 numpy scipy matplotlib pandas seaborn scikit-learn h5py pillow tqdm
conda install -n Tensorflow2.0 tensorflow-gpu 

pip install gast==0.2.2
pip install tensorboard==2.1.0

python -m ipykernel install --user --name Tensorflow2.0 --display-name "Tensorflow2.0"
# jupyter kernelspec uninstall Tensorflow2.0 
# jupyter kernelspec list
